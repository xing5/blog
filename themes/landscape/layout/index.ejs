<!DOCTYPE html>
<html lang="en" class="no-js">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge"> 
        <meta name="viewport" content="width=device-width, initial-scale=1"> 
        <title><%= config.title %></title>
        <meta name="description" content="Examples for creative website header animations using Canvas and JavaScript" />
        <meta name="keywords" content="header, canvas, animated, creative, inspiration, javascript" />
        <meta name="author" content="Codrops" />
        <link rel="shortcut icon" href="/favicon.ico">
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
        <!--[if IE]>
        <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
      <link href="//fonts.googleapis.com/css?family=Raleway:200,400,800|Inconsolata:400,700|Open+Sans:700,400" rel="stylesheet" type="text/css">
      <%- css('css/style') %>
        <link rel="stylesheet" type="text/css" href="css/normalize.css" />
        <link rel="stylesheet" type="text/css" href="css/component.css" />
      <%- partial('_partial/google-analytics') %>
    </head>
    <body>
        <div id='container' class="container intro-effect-push">
            <div class="content">
                <div id="large-header" class="large-header">
                    <canvas id="demo-canvas"></canvas>
                    <div id='title-wrapper'>
                        <div class="half">
                            <h1 id="title" class="main-title">Xing <span class="thin">Wu</span></h1>
                        </div>
                        <div class="half">
                            <div id="json-desc" class="title-desc">
                                <div class="desc-block">
                                    <span class="brace">{</span>
                                </div>
                                <div class="desc-block json">
<pre>
"<span class="hljs-attribute">location</span>": <span class="hljs-value">"Montreal, CA"</span>,
"<span class="hljs-attribute">occupation</span>": <span class="hljs-value">"programmer"</span>,
"<span class="hljs-attribute">birthday</span>": <span class="hljs-number">573125400</span>,
"<span class="hljs-attribute">pronunciation</span>": <span class="hljs-value">"/ʃɪŋ-wu:/"</span>
</pre>

                                </div>
                                <div class="desc-block">
                                    <span class="brace">}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id='nav-wrapper'>
                        <nav class="links" id='desc-links'>
                            <a href="https://twitter.com/xing101" target='_blank' class="btn-nav"> <i class="fa social-icon fa-twitter"></i></a>
                            <a href="https://www.facebook.com/wuxing" target='_blank' class="btn-nav"><i class="fa social-icon fa-facebook"></i></a>
                            <a href="http://instagram.com/calka" target='_blank'  class="btn-nav"><i class="fa social-icon fa-instagram"></i></a>
                            <a href="https://www.linkedin.com/in/xingwu" target='_blank'  class="btn-nav"><i class="fa social-icon fa-linkedin"></i></a>
                            <a href="http://resume.xingwu.me" target='_blank'  class="btn-nav"><span>CV</span></a>
                        </nav>
                    </div>
                    <button id='scroll-btn' class="trigger" data-info='Blog'><i class="fa fa-chevron-down"></i></button>
                </div>

                <div class="main-wrapper outer">
                <section id="main"><%- partial('_partial/archive', {pagination: 2, index: true}) %></section>
                <% if (theme.sidebar && theme.sidebar !== 'bottom'){ %>
                  <% if (page.toc === true){ %>
                    <%- partial('_partial/sidebar-toc') %>
                  <% } else {%>
                    <%- partial('_partial/sidebar', null, {cache: !config.relative_link}) %>
                  <% } %>
                <% } %>
                <%- partial('_partial/footer', null, {cache: !config.relative_link}) %>
                </div>
            </div>
            <%- partial('_partial/after-footer') %>
        </div>
        <script src="js/TweenLite.min.js"></script>
        <script src="js/rAF.js"></script>
        <script src="js/classie.js"></script>
        <script src="js/matrix.js"></script>
        <script src="js/scroll.js"></script>
        <script src="js/script.js"></script>
    </body>
</html>
